{
  "name": "@cloudflare/ai",
  "version": "1.0.36",
  "description": "JavaScript library for Cloudflare's Workers AI service",
  "author": "Sven Sauleau <sven@cloudflare.com>",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "scripts": {
    "build": "tsc && yarn replace-version",
    "build:for-dashboard": "npm_package_version=$npm_package_version bash ./scripts/build-for-dashboard.sh",
    "prepublish": "bash ./scripts/add_registry_config.sh && yarn build",
    "test": "NODE_NO_WARNINGS=1 NODE_OPTIONS=\"--loader ts-node/esm\" mocha --config ./mocharc.js ./test/sdk",
    "test-rest": "NODE_NO_WARNINGS=1 NODE_OPTIONS=\"--loader ts-node/esm\" mocha --config ./mocharc.js ./test/rest",
    "dry-publish": "npm pack --dry-run",
    "replace-version": "find ./dist/ -type f -name \"*.js\" -print0 | xargs -0 sed -i -e \"s/@version@/$npm_package_version/g\""
  },
  "files": [
    "dist/*"
  ],
  "devDependencies": {
    "@types/mocha": "^10.0.3",
    "@types/mustache": "^4.2.4",
    "@types/node": "^20.3.3",
    "ajv": "^8.12.0",
    "dotenv": "^16.3.1",
    "mocha": "^10.2.0",
    "mustache": "^4.2.0",
    "prettier": "^2.8.8",
    "ts-node": "^10.9.1",
    "tsup": "^7.2.0",
    "typescript": "^5.1.6"
  }
}
